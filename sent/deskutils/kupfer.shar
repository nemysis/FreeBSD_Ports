XUSE_GNOME=	desktopfileutils gvfs intltool pygtk2 pygnomedesktop
XUSE_PYTHON=	yes
XINSTALLS_ICONS=	yes
X
XMAN1=		kupfer.1
XMLINKS=		kupfer.1 kupfer-exec.1
X
XPORTDOCS=	*
X
XDOCSRCDIR1=	${WRKSRC}
XDOC_FILES1=	README.rst
X
XDOCSRCDIR2=	${WRKSRC}/Documentation
XDOCSDIR2=	${DOCSDIR}/Documentation
XDOC_FILES2=	Contributing.rst GTKTheming.rst PluginAPI.rst VersionHistory.rst
X
XOPTIONS_DEFINE=	NLS NAUTILUS RHYTHMBOX
XNAUTILUS_DESC=	Enable Nautilus support
XRHYTHMBOX_DESC=	Enable Rhythmbox plugins support
X
X.include <bsd.port.options.mk>
X
X.if ${PORT_OPTIONS:MNLS}
XUSE_GETTEXT=	yes
XPLIST_SUB+=	NLS=""
X.else
XPLIST_SUB+=	NLS="@comment "
X.endif
X
X.if ${PORT_OPTIONS:MNAUTILUS}
XRUN_DEPENDS+=	${LOCALBASE}/lib/nautilus-python/nautilus:${PORTSDIR}/x11-fm/py-nautilus
X.endif
X
X.if ${PORT_OPTIONS:MRHYTHMBOX}
XRUN_DEPENDS+=	rhythmbox:${PORTSDIR}/audio/rhythmbox
X.endif
X
Xpost-patch:
X	@${REINPLACE_CMD} -e 's|--quiet||' ${WRKSRC}/bin/kupfer.in
X	@${REINPLACE_CMD} -e 's|locale.bindtextdomain|gettext.bindtextdomain|' \
X		-e 's|locale.bind_textdomain_codeset|gettext.bind_textdomain_codeset|' \
X		${WRKSRC}/kupfer/main.py
X
X.if ! ${PORT_OPTIONS:MNLS}
X	@${REINPLACE_CMD} -e '/^build_subdirs/s|po||g' ${WRKSRC}/wscript
X.endif
X
Xdo-configure:
X	@cd ${WRKSRC} && ${SETENV} ${MAKE_ENV} ./waf configure --mandir=${PREFIX}/man
X
Xdo-build:
X	@cd ${WRKSRC} && ${SETENV} ${MAKE_ENV} ./waf build
X
Xdo-install:
X	@cd ${WRKSRC} && ${SETENV} ${MAKE_ENV} ./waf install
X
Xpost-install:
X.if ${PORT_OPTIONS:MDOCS}
X	@${MKDIR} ${DOCSDIR}
X	${INSTALL_DATA} ${DOC_FILES1:S|^|${DOCSRCDIR1}/|} ${DOCSDIR}
X	@${MKDIR} ${DOCSDIR2}
X	${INSTALL_DATA} ${DOC_FILES2:S|^|${DOCSRCDIR2}/|} ${DOCSDIR2}
X.endif
X
X	${INSTALL_MAN} ${WRKSRC}/build/${MAN1} ${MAN1PREFIX}/man/man1
X
X	@-update-desktop-database -q
X	@-update-mime-database ${PREFIX}/share/mime
X
X.include <bsd.port.mk>
