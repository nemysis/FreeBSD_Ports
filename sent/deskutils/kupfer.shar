X
XLICENSE=	GPLv3
X
XBUILD_DEPENDS=	rst2html:${PORTSDIR}/textproc/py-docutils \
X		xml2po:${PORTSDIR}/textproc/gnome-doc-utils \
X		update-mime-database:${PORTSDIR}/misc/shared-mime-info
XRUN_DEPENDS=	${PYTHON_PKGNAMEPREFIX}dbus>=0:${PORTSDIR}/devel/py-dbus \
