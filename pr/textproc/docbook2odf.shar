X		${XARGS} -0 ${RM} -rf
X
Xpost-patch:
X	@${REINPLACE_CMD} -e 's|#!/usr/bin/perl|#!${PERL}|' \
X		${WRKSRC}/tests/tests/docbook-43.001.xml ${WRKSRC}/utils/maketests \
X		${WRKSRC}/utils/${PORTNAME}
X	@${RM} ${WRKSRC}/tests/tests/*.bak
X	@(cd ${WRKSRC}/utils && ${RM} *.bak)
X
Xdo-install:
